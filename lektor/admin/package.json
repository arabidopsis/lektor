{
  "name": "lektor",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@openfonts/roboto-slab_all": "^1.0.1",
    "sweetalert2": "^10.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "bootstrap": "~3.3.0",
    "chai": "^4.2.0",
    "css-loader": "^4.3.0",
    "event-source-polyfill": "^1.0.20",
    "file-loader": "^6.1.0",
    "font-awesome": "^4.3.0",
    "jquery": "^3.5.1",
    "jsdomify": "^3.1.1",
    "json-loader": "^0.5.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.12.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "standard": "^14.3.4",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "lint": "standard",
    "fix-lint": "standard --fix",
    "test": "nyc mocha static/js/**/*.test.js",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov",
    "dev": "webpack --watch --config ./static/webpack.config.js --context ./static",
    "webpack": "webpack --config ./static/webpack.config.js --context ./static",
    "production": "webpack --mode production --config ./static/webpack.config.js --context ./static"
  },
  "targets": "defaults",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "static/gen/"
    ],
    "globals": [
      "$LEKTOR_CONFIG"
    ]
  },
  "author": "",
  "license": "ISC"
}
